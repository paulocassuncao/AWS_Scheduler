@model IEnumerable<AWS_Scheduler.Models.Credentials>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CredentialsName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccessKeyId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SecretAccessKey)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CredentialsName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AccessKeyId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecretAccessKey)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
